<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="op.AdvertisingVersionEntity" table="advertisingVersion" schema="cottee">
        <id name="id">
            <column name="id" sql-type="int(10) unsigned"/>
        </id>
        <property name="version">
            <column name="version" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="createTime">
            <column name="createTime" sql-type="date"/>
        </property>
    </class>
</hibernate-mapping>